{
  "manifest_version": 3,
  "name": "EDScrapper",
  "version": "1.0",

  "icons": {
    "128": "src/img/icon128.png"
  },

  "permissions": [
    "activeTab",
    "scripting",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "src/devtool/devtools.html",
  "background": {
    "service_worker": "background.js"
  }
}
